{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "Hangfire": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/debug.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine:1}{Exception}",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15,
          "fileSizeLimitBytes": 52428800,
          "buffered": true,
          "rollOnFileSizeLimit": true,
          "flushToDiskInterval": "00:00:05"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp} [{Level}] {Message}{NewLine:1}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "Zowari"
    },
    "IdentityConfiguration": {
      "Secret": "Ij3dda9H899@W^Lxk!fQE7OiOy&hI45g",
      "ValidIssuer": "Zowari",
      "ValidAudience": "Zowari",
      "Expires": 60
    }
  }
}